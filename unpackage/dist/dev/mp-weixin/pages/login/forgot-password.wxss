/* ==================
        初始化
 ==================== */
page {
	background-color: #fff;
	font-size: 28rpx;
	color: #333;
	line-height: 1.6;
	font-family: "mp-quote", -apple-system-font, BlinkMacSystemFont, "Helvetica Neue", "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei UI", "Microsoft YaHei", Arial, sans-serif;
}
view,scroll-view,swiper,swiper-item,cover-view,cover-image,icon,text,rich-text,progress,button,checkbox,form,input,label,radio,slider,switch,textarea,navigator,audio,camera,image,video{box-sizing: border-box;}
switch { -webkit-transform: translateX(16rpx) scale(0.9); transform: translateX(16rpx) scale(0.9);
}
/**
 * 页面公共样式
 **/
input{height: inherit;}
uni-button, button {font-weight: normal;}
uni-button.no-border:before, uni-button.no-border:after, button.no-border:before, button.no-border:after {border: 0 none;}
button{border: 0 none;}
button.btn {background-color: transparent;padding-left: 0;padding-right: 0; height: inherit; line-height:inherit;}
view[class*='-area']{
}
view[class*='use-area']{ border-radius: 20rpx; background: #fff; padding: 30rpx; margin-top: 20rpx;
}
view[class*='-round-area']{ border-radius: 20rpx;
}
.safe-area-inset-bottom {padding-bottom: 0; padding-bottom: constant(safe-area-inset-bottom);padding-bottom: env(safe-area-inset-bottom);}
.safe-area-inset-bottom-plus {margin-bottom: 0; margin-bottom: constant(safe-area-inset-bottom);margin-bottom: env(safe-area-inset-bottom);}
.use-page { min-height: 100%; width: 100%; background: #fff;
}
.use-hover-class { opacity: 0.6;
}
.use-btn {background-color: #ff6a6c; padding: 16rpx 60rpx; border-radius: 60rpx; color: #fff;}
/* 三角形 */
.triangle-up:after { content: ' ';width: 0;height: 0;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-bottom: 10px solid #fff;
	position: absolute;top: -8px;right: 7px;}
.triangle-down:after {
	content: ' ';width: 0;height: 0;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-top: 10px solid #fff;
	position: absolute;top: -8px;right: 7px;}
.triangle-b::after {
	content: ' ';
	width: 0;
	height: 0;
	border-left: 6px solid transparent;
	border-right: 6px solid transparent;
	
    border-top: 7px solid #ff6a6c;
}
.scale-08{-webkit-transform: translateX(-20rpx) scale(0.8);transform: translateX(-20rpx) scale(0.8);}
.scale-09{-webkit-transform: translateX(-14rpx) scale(0.9);transform: translateX(-14rpx) scale(0.9);}
/**
 * 组件公共样式
 **/
.price, .price-jf, .discount {font-size: 36rpx; color: #ff6a6c; line-height: 1; font-weight: 580;}
.m-price{font-size: 24rpx;text-decoration: line-through;color: #909399;margin-left: 20rpx;}
.price::before{ content: '￥'; font-size: 24rpx;
}
.m-price::before{ content: '￥'; font-size: 24rpx;
}
.price::after{ content: attr(data-decimal); font-size: 24rpx;
}
.price-add::before { content: '＋'; font-weight: 600;
}
.price-sub::before { content: '－'; font-weight: 600;
}
.price-jf::after { content: '积分'; font-size: 22rpx; margin-left: 6rpx;
}
.badge{ position: absolute; top: 0; right: 0;background: #ff6a6c; color: #fff;font-size: 12px;	line-height: 1;display: inline-block;padding: 3px 6px;border-radius: 50px;font-weight: normal !important;}
.badge-small{-webkit-transform: scale(0.8);transform: scale(0.8);-webkit-transform-origin: center center;transform-origin: center center;}
.discount::after { content: '折'; font-size: 24rpx; margin-left: 6rpx;
}
.dn{ display: none !important;
}
.diblock{ display: inline-block;
}
/* flex 布局 */
.dflex{display: flex; align-items: center;}
.dflex-s{display: flex; align-items: flex-start;}
.dflex-e{display: flex; justify-content: flex-end; align-items: baseline;}
